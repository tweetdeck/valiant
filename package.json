{
  "name": "valiant",
  "description": "JavaScript interval arithmetic library",
  "main": "out.js",
  "files": [
    "out.js",
    "out.js.map"
  ],
  "dependencies": {
    "babel-runtime": "^6.11.6"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-dead-code-elimination": "^2.1.0",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "ghooks": "^1.3.2",
    "semantic-release": "^4.3.5",
    "validate-commit-message": "^3.0.1",
    "xo": "^0.16.0"
  },
  "scripts": {
    "test": "xo && NODE_ENV=test babel-node index.js",
    "dist": "NODE_ENV=production babel index.js --source-maps --out-file out.js",
    "prepublish": "npm run dist",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "ghooks": {
      "pre-commit": "xo",
      "commit-msg": "validate-commit-msg",
      "pre-push": "npm test"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-runtime",
          "transform-inline-environment-variables",
          "transform-dead-code-elimination",
          "add-module-exports"
        ]
      }
    }
  },
  "xo": {
    "esnext": true,
    "space": true,
    "ignores": [
      "out.js"
    ]
  },
  "keywords": [],
  "contributors": [
    "Tom Ashworth <ta@twitter.com>",
    "Sol Plant"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tweetdeck/valiant.git"
  }
}
